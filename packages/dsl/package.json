{
  "name": "@ai-builder/dsl",
  "version": "0.1.0",
  "description": "Core DSL package for AI Builder MDA toolchain",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.mjs",
      "require": "./dist/primitives/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.mjs",
      "require": "./dist/ui/index.js"
    },
    "./utils/metadata": {
      "types": "./dist/utils/metadata.d.ts",
      "import": "./dist/utils/metadata.mjs",
      "require": "./dist/utils/metadata.js"
    },
    "./extension": {
      "types": "./dist/extension/index.d.ts",
      "import": "./dist/extension/index.mjs",
      "require": "./dist/extension/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && pnpm build:dts",
    "build:dts": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/**",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "dsl",
    "mda",
    "typescript",
    "decorators"
  ],
  "author": "AI Builder Team",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "glob": "^10.0.0",
    "ts-morph": "^21.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  }
}
