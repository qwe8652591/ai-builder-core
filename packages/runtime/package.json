{
  "name": "@ai-builder/runtime",
  "version": "0.1.0",
  "description": "Runtime implementation for AI Builder DSL",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup && pnpm build:dts",
    "build:dts": "tsc src/index.ts --declaration --emitDeclarationOnly --outDir dist --skipLibCheck --moduleResolution bundler --module esnext --target es2022",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@ai-builder/dsl": "workspace:*",
    "decimal.js-light": "^2.5.1",
    "mitt": "^3.0.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
