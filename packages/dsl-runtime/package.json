{
  "name": "@qwe8652591/dsl-runtime",
  "version": "0.1.9",
  "description": "DSL Runtime - 解析和运行 DSL 项目",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "dsl-runtime": "./dist/cli.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts dev",
    "build": "tsup",
    "start": "tsx src/cli.ts start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwe8652591/ai-builder-core.git",
    "directory": "packages/dsl-runtime"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@qwe8652591/dsl-core": "workspace:*",
    "@qwe8652591/std-ui": "workspace:*",
    "@vitejs/plugin-react": "^4.3.0",
    "vite": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "antd": "^5.12.0",
    "@ant-design/icons": "^5.2.0",
    "dayjs": "^1.11.10",
    "sql.js": "^1.13.0",
    "glob": "^10.3.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  }
}
