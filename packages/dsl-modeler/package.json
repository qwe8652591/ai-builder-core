{
  "name": "@qwe8652591/dsl-modeler",
  "version": "0.1.3",
  "description": "DSL Modeler - 元数据建模工作台，可独立运行或集成到项目中",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "bin": {
    "dsl-modeler": "./dist/cli.js"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts dev",
    "dev:watch": "tsup --watch",
    "start": "tsx src/cli.ts",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "dsl",
    "modeler",
    "metadata",
    "designer",
    "workbench",
    "cli"
  ],
  "author": "AI Builder Team",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^5.2.0",
    "@qwe8652591/dsl-core": "workspace:*",
    "@qwe8652591/std-ui": "workspace:*",
    "@qwe8652591/vite-plugin": "workspace:*",
    "@vitejs/plugin-react": "^4.3.0",
    "antd": "^5.12.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "glob": "^10.3.0",
    "mermaid": "^11.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.7.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwe8652591/ai-builder-core.git",
    "directory": "packages/dsl-modeler"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
