{
  "name": "@ai-builder/std-ui",
  "version": "0.1.0",
  "description": "Standard UI component protocol for AI Builder DSL - framework-agnostic component interfaces",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && pnpm build:dts",
    "build:dts": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/**",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "dsl",
    "ui",
    "components",
    "typescript",
    "framework-agnostic",
    "vue",
    "react"
  ],
  "author": "AI Builder Team",
  "license": "MIT",
  "dependencies": {
    "@ai-builder/ui-types": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {
    "@ai-builder/dsl": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}

